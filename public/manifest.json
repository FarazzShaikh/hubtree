{
    "manifest_version": 2,
    "name": "Hubtree",
    "author": "tobe",
    "version": "1.0.0",
    "description": "View a directory tree for github repositories",
    "permissions": [ "https://github.com/*"],
    "background": {
      "scripts": ["background.js"]
    },
    "icons": { 
        "16": "images/icon16.png",
        "48": "images/icon128.png",
        "128": "images/icon128.png"
     },
    "browser_action": {
        "default_icon": {
          "16": "images/icon16.png"     
        },
        "default_popup": "popup.html"     
      }
}